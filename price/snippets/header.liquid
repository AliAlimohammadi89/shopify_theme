    <div class="header-top-menu">
       
      
      <ul class="header_user">
    {% if customer %}
         <div class="header-top-menu-items">
          <a href="/account">{{ customer.first_name | truncate:15 }}</a>
        </div>
         <div class="header-top-menu-items">
          <a href="/account/logout">
           Log out</a>
        </div>
    {% else %}
         <div class="header-top-menu-items">
          <a href="/account/login">
           Log in</a></div>
             <div class="header-top-menu-items"><a href="/account/register">
               Create an account</a></div>
    {% endif %}
</ul>
    </div>
</div>
<div class="header-bottom">
