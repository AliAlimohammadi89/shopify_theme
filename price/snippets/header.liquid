                    {% for sublink in linklists.[link.handle].links %}
                 	   <div>
                         <a href="{{ sublink.url }}">{{ sublink.title | escape }}</a>
                 		</div>
                    {% endfor %}
                  {% endif %} 
                   
                </div>
            </div>
           {% endfor %}

        </div>
    </div>
  
  
